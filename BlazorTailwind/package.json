{
  "devDependencies": {
    "cross-env": "^7.0.3",
    "tailwindcss": "^3.4.17"
  },
  "scripts": {
    "tailwind": "cross-env NODE_ENV=development ./node_modules/tailwindcss/lib/cli.js -i ./Styles/tailwind.css -o ./wwwroot/css/tailwind.css --watch",
    "tailwind-prod": "npx tailwindcss -i ./Styles/tailwind.css -o ./wwwroot/css/tailwind.css --minify"
  }
}
